{
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "arrow-parens": "off",
    "no-return-assign": "off",
    "no-constant-condition": "off",
    "no-confusing-arrow": "off"
  },
  "env": {
    "browser": true
  }
}
